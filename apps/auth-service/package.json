{
  "name": "@ubereats/auth-service",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Victor Onofiok",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "nodemon --exec NODE_ENV=dev ts-node src/server.ts",
    "db:generate": "drizzle-kit generate:pg --config=./src/config/db/drizzle.config.ts",
    "db:push": "drizzle-kit push:pg",
    "db:preview": "yarn drizzle-kit studio --config=./src/config/db/drizzle.config.ts",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.3",
    "@types/randomatic": "^3.1.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "randomatic": "^3.1.1",
    "ubereats-types": "*",
    "tsconfig": "*",
    "ubereats-cache-pkg": "*",
    "mq-service-pkg": "*"
  }
}
